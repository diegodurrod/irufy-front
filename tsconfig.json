{
  "extends": "@quasar/app/tsconfig-preset",
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "preserve",
    "moduleResolution": "node",
    "baseUrl": ".",
    "esModuleInterop": true,
    "types": [
      "jest",
      "quasar"
